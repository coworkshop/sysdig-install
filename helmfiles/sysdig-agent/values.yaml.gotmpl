global:
  clusterConfig:
    name: {{ .Values.clusterName }}
  sysdig:
    accessKey: {{ .Values.sysdig.accessKey }}
agent:
  sysdig:
    settings:
      collector: ingest-{{ .Values.sysdig.url }}
      collector_port: 6443
nodeAnalyser:
  nodeAnalyser:
    apiEndpoint: {{ .Values.sysdig.url }}
    secure:
      vulnerabilityManagement:
        newEngineOnly: true
